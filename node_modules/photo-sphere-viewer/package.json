  "main": "dist/photo-sphere-viewer.js",
  "types": "dist/photo-sphere-viewer.d.ts",
  "files": [
    "src/",
    "dist/",
    "types/"
  ],
  "authors": [
    {
      "name": "Jérémy Heleine",
